{
	"name": "vscode-parinfer",
	"description": "Parinfer for VS Code",
	"version": "0.0.1",
	"publisher": "joaomoreno",
	"icon": "icon.png",
	"engines": {
		"vscode": "0.10.x"
	},
	"activationEvents": [
		"onLanguage:clojure"
	],
	"main": "./extension",
	"contributes": {
		"keybindings": [{
			"command": "parinfer.toggleMode",
			"key": "Ctrl+R"
		}],
		"commands": [{
			"command": "parinfer.toggleMode",
			"title": "Toggle Parinfer Mode"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	},
	"license": "MIT"
}